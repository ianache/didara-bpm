<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-cascade="all" 
    package="com.bpm4sb.process.domain.model.taskedefinition">
    <class name="TaskDefinition" table="TASKDEFINITION" lazy="true">
        <id name="id" type="java.lang.String" column="ID"/>
        <property name="title" column="TITLE" type="java.lang.String" />
        <property name="instanceTitle" column="INSTTITLE" type="java.lang.String" />
        <property name="participantId" column="PARTICIPANT_ID" type="java.lang.String" />
        <property name="expirationExpression" column="EXPEXPRESSION" type="java.lang.String" />
        <property name="skipable" type="java.lang.Boolean" column="SKIPABLE" />
        <property name="priority" column="PRIORITY" type="java.lang.Integer" />
    </class>
</hibernate-mapping>